<form id="signup" class="well user-form">
	<h2>Signup</h2>
	
	<label>Email</label>
	<input name="email" type="text" class="span3" placeholder="">
	
	<label>Password</label>
	<input name="password" type="password" class="span3" placeholder="" >
	
	<label>Confirm Password</label>
	<input name="password-confirm" type="password" class="span3" placeholder="" >
	
	<button type="submit" class="btn">
		Submit
	</button>
</form>
